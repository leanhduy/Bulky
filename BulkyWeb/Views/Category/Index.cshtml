@model List<Category>

<h1>Category List</h1>
<table class="table table-bordered table-striped">
	<thead>
		<tr>
			<th>Category Name</th>
			<th>Display Order</th>
		</tr>
	</thead>
	<tbody>
		@foreach(var cate in Model){
			<tr>
				<td>@cate.Name</td>
				<td>@cate.DisplayedOrder</td>
			</tr>
		}
	</tbody>
</table>